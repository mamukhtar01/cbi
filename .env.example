# SQLite database file path (relative to the prisma/ directory)
DATABASE_URL=file:./dev.db

# NextAuth configuration
# NEXTAUTH_URL must match the URL you run the app on (e.g. http://localhost:3000)
NEXTAUTH_URL=http://localhost:3000
# Generate a random secret: openssl rand -base64 32
NEXTAUTH_SECRET=your_nextauth_secret

# Seed admin credentials (used by `npm run db:seed`)
SEED_ADMIN_EMAIL=admin@admin.com
SEED_ADMIN_PASSWORD=admin@123
SEED_ADMIN_NAME=admin

# Simulated failure/delay rates expressed as whole percentages (0â€“100)
# e.g. 10 means a 10% chance of failure or delay
TELECOM_DELAY_RATE=10
TELECOM_FAIL_RATE=10
WAVE_FAIL_RATE=10
